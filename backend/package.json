{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --config jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config.js --testPathPattern=e2e",
    "test:unit": "jest --config jest.config.js --testPathPattern=spec",
    "test:validation": "jest --config jest.config.js --testPathPattern=validation",
    "test:flow": "jest --config jest.config.js --testPathPattern=flow",
    "test:statistics": "jest --config jest.config.js --testPathPattern=statistics",
    "test:services": "jest --config jest.config.js --testPathPattern=service",
    "test:services:watch": "jest --config jest.config.js --testPathPattern=service --watch",
    "test:services:cov": "jest --config jest.config.js --testPathPattern=service --coverage",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "test:db": "ts-node src/config/database/testConnection.ts",
    "test:performance": "ts-node src/config/database/performanceTest.ts",
    "monitor:db": "ts-node src/config/database/monitor.ts",
    "migrations:run": "npm run typeorm migration:run -- -d ./database/dataSource.ts",
    "migrations:create": "npm run typeorm -- migration:create ./database/migrations/migrations",
    "migrations:revert": "npm run typeorm -- -d ./database/dataSource.ts migration:revert",
    "migrations:generate": "npm run typeorm -- -d ./database/dataSource.ts migration:generate ./database/migrations/migrations",
    "seed:all": "ts-node ./database/typeorm.seed.ts",
    "seed:employee": "ts-node ./database/seeders/Employee.seeder.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.50.5",
    "@types/qrcode": "^1.5.5",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.6.1",
    "handlebars": "^4.7.8",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "puppeteer": "^24.10.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.24",
    "typeorm-extension": "^3.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.33.1"
  }
}
